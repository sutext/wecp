.wrapper{
  width: 100%;
  height: 100%;
}
.common{
  all: unset;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.common::after{
  all: unset;
}
.line{
  box-sizing: border-box;
  background-color: white;
}
.column{
  flex-direction: column;
}
.hover{
  filter: brightness(80%);
}