<view class="wrapper" style="z-index: {{zindex}};" wx:if="{{isShow}}" catch:touchmove="_ignore" catch:tap="dismiss">
    <view class='blur' style='opacity:{{opacity}};' />
    <view class="content" style="{{ctstyle}}position: absolute;height:{{ctheight}};{{position}}:{{ctoffset}};"
        catch:tap="_ignore">
        <slot />
    </view>
</view>